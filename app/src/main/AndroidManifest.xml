        <activity android:name=".ActivityCustomizarLayout">
        </activity>
        <activity android:name=".ActivityDesempenho">
        </activity>
        <activity android:name=".ActivityExercicio">
        </activity>
        <activity android:name=".ActivityCursosCadastrados">
        </activity>
